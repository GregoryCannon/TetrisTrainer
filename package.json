{
  "homepage": "https://gregorycannon.github.io/TetrisTrainer/",
  "name": "tetrisjs",
  "version": "1.0.0",
  "description": "tetris trainer",
  "main": "src/index.js",
  "scripts": {
    "deploy": "npm run make && gh-pages -d docs",
    "start": "prettier src/*.js --write && webpack --progress --mode=development && webpack-dev-server",
    "make": "prettier src/*/*.js --write && webpack --progress --mode=production",
    "format": "prettier --write \"src/**/*.+(js|jsx|ts|json|css|md)\""
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "webpack": "^5.88.2",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1"
  },
  "dependencies": {
    "node-fetch": "^3.3.2",
    "prettier": "^3.0.3"
  }
}
