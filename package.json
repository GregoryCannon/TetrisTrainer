{
  "homepage": "https://gregorycannon.github.io/TetrisTrainer/",
  "name": "tetrisjs",
  "version": "1.0.0",
  "description": "tetris trainer",
  "main": "src/index.js",
  "scripts": {
    "deploy": "npm run make && gh-pages -d docs",
    "start": "prettier src/*.js --write && webpack --progress --mode=development && webpack serve",
    "make": "prettier src/*.js --write && webpack --progress --mode=production",
    "format": "prettier --write \"src/**/*.+(js|jsx|json|css|md)\""
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "gh-pages": "^3.1.0",
    "webpack": "^5.90.1",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1"
  },
  "dependencies": {
    "prettier": "^3.2.5"
  }
}
